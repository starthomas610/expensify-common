{
  "name": "js-libs",
  "version": "2.5.8",
  "description": "Expensify libraries and components shared across different repos",
  "main": "index.js",
  "license": "UNLICENSED",
  "scripts": {
    "grunt": "grunt",
    "test": "jest",
    "patch": "npm --no-git-tag-version version patch",
    "update-tlds": "curl -s https://data.iana.org/TLD/tlds-alpha-by-domain.txt | sed '1d' | awk '{print length, $0}' - | sort -n -r | cut -d \" \" -f2- | tr '\\n' '|' | sed s'/.$//' | printf \"const TLD_REGEX='$(cat -)';\\n\\nexport default TLD_REGEX;\\n\" > lib/tlds.jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Expensify/JS-Libs.git"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "clipboard": "2.0.4",
    "jquery": "3.3.1",
    "lodash.get": "4.4.2",
    "lodash.has": "4.5.2",
    "moment": "2.20.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "simply-deferred": "3.0.0",
    "underscore": "1.9.1"
  },
  "devDependencies": {
    "babel-jest": "^26.3.0",
    "eslint-config-expensify": "2.0.7",
    "grunt": "*",
    "grunt-eslint": "22.0.0",
    "grunt-chokidar": "1.0.2",
    "@babel/preset-env": "^7.0.0",
    "jest": "^26.4.2",
    "jit-grunt": "^0.10.0"
  }
}
